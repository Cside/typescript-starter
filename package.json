{
  "scripts": {
    "build": "tsc && yarn run babel",
    "watch": "tsc-watch --onSuccess 'yarn run babel'",
    "babel": "babel dist --out-dir dist",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --write . '!dist/**/*'",
    "lint": "yarn run eslint:fix && yarn run prettier"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@types/jest": "^29.1.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "core-js": "3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.2.0",
    "jsdom": "^20.0.1",
    "prettier": "^2.7.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.4",
    "ts-jest": "^29.0.3"
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": [
        "ts-jest"
      ]
    }
  }
}
